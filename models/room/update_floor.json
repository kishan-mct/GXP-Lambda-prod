{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "FloorUpdate",
    "type": "object",
    "properties": {
        "floor_name": {
            "type": "string",
            "maxLength": 200
        },
        "short_name": {
            "type": "string",
            "maxLength": 50
        },
        "total_room": {
            "type": "integer",
            "minimum": 0,
            "default": 0
        },
        "is_active": {
            "type": "boolean",
            "default": true
        }
    },
    "additionalProperties": false
}